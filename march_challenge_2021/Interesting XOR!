


        /////                      TLE



#include<bits/stdc++.h>
//#include<conio>
using namespace std;

int calc(long n)
{
  long k;
  for(int i=1;i<n;i++)
  {
    if(pow(2,i)>n)
    { 
      k=pow(2,i); break;
    }
    
  }
  return k;
}

int main()
{
 int t; cin>>t;
 while(t--)
 {
  long n;
  cin>>n;
  long d=calc(n);
  long max=0;
  for(int i=1;i<d;i++)
  {
    for(int j=1;j<d;j++)
    {
      if((i^j)==n)
      {
        if((i*j)>max)
          max=i*j;
       // cout<<i<<" "<<j<<endl;}
      }
    }
  }
  cout<<max<<endl;
 }
  return 0;
}


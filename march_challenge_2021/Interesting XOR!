


        ///                    TLE in one cases



#include<bits/stdc++.h>
//#include<conio>
using namespace std;

int calc(long n)
{
  long k=2;
  for( k=2;k<=n;k=k*2);
  
  return k;
}

int main()
{
 int t; cin>>t;
 while(t--)
 {
  long n;
  cin>>n;
  long d=calc(n);
 // cout<<d<<endl;
  long max=0;
  long a=(d/2)-1;
  if(n==1)
    cout<<"1"<<endl;
  else
  {  
  for(int i=d-1;i>0;i--)
  {
     if((a^i)==n)
     {
     // cout<<a<<" "<<i<<endl;
       max=a*i;
       break;
     }
  }  
  cout<<max<<endl;
 }
}
  return 0;
}
